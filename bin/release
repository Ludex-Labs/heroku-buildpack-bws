#!/usr/bin/env bash

# The absolute path to the root of the buildpack.
BUILDPACK_DIR=$(cd "$(dirname "$(dirname "${BASH_SOURCE[0]}")")" && pwd)

# Source the environment variables file
source "${BUILDPACK_DIR}/.profile.d/bws_secrets.sh"

# Ensure the secrets file is removed at the end of the script
trap 'rm -f "${BUILDPACK_DIR}/.profile.d/bws_secrets.sh"' EXIT

exit 0
